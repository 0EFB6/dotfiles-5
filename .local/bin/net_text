SSID=$(iwgetid -r)
interface=$(ip route get 8.8.8.8 | cut -d" " -f5 | head -1)

if [[ $interface = "wlo1" ]]
then
	printf "%s" "$SSID"
elif [[ $interface = "eno1" ]] 
then
	printf "%s" "$SSID"
fi
