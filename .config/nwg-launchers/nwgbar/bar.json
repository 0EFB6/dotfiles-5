[
  {
    "name": "Lock screen",
    "exec": "betterlockscreen -l dimblur",
    "icon": "/usr/share/icons/WhiteSur-dark/actions/32/system-lock-screen.svg"
  },
  {
    "name": "Logout",
    "exec": "loginctl terminate-session self",
    "icon": "/usr/share/icons/WhiteSur-dark/actions/32/system-log-out.svg"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "/usr/share/icons/WhiteSur-dark/actions/32/system-reboot.svg"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "/usr/share/icons/WhiteSur-dark/actions/32/system-shutdown.svg"
  }
]
